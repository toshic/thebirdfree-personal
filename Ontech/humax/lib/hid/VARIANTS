
VARIANTS := device device_debug host host_debug

CFLAGS_device := -DHID_DEVICE							# flags for device build of HID lib
CFLAGS_device_debug := -DHID_DEBUG_LIB -DHID_DEVICE		# flags for debug version of HID lib
CFLAGS_host := -DHID_HOST								# flags for host build of HID lib
CFLAGS_host_debug := -DHID_DEBUG_LIB -DHID_HOST			# flags for debug host build of HID lib
